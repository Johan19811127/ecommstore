import mongoose from 'mongoose';

const TransactionSchema = new mongoose.Schema(
  {
    transactDate:{
        type: Date, default: Date.now ,
        required:true
    },

    transactType:{
        type:String,
        required:true,
    },
    with:{
        type
    }
});

export default mongoose.models.Transaction || mongoose.model('Transaction', TransactionSchema);


